<% if current_user %>
  <%= link_to "Dashboard", dashboard_path %>
  <div class="users">
    <ul><% @users.each do |user| %>
      <li><%= user.email %></li>
    <% end %></ul>
  </div>
  <%= link_to "Log Out", logout_path %><br>
<% else %>
  <%= link_to "Create a New User", new_user_path, method: :get %><br>
  <%= link_to "I already have an account", login_path %>
<% end %>


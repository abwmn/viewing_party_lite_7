<% if current_user %>
  <%= link_to "Log Out" %>
<% else %>
  <%= button_to "Create a New User", new_user_path, method: :get %>
  <%= link_to "I already have an account", login_path %>
<% end %>
<div class="users">
  <ul><% @users.each do |user| %>
    <li><%= link_to "#{user.name}", user_path(user) %></li>
  <% end %></ul>
</div>

